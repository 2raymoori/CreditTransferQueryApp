  <style>
  body{
    background-color:white;
      background-position: center; /* Center the image */
  background-repeat: no-repeat; /* Do not repeat the image */
  background-size:cover; /* Resize the background image to cover the entire container */
  }

    #main{
    }
    section{
         padding-top:2%;
    }
  </style>
</head>
<body>
  <main id="main" >
  <section class="container">
  
<%//action="/api/transferres"%>
<form target="_blank" id="formSbt"  method="post">
    <div class="row">
    <div class="col-lg-12">
    <div id="imgCont">
  <div>
    <img src="/images/index.png" width="50"/>
  </div>

</div>

<div class="row form-group">
    <div class="col-lg-3">
      <label for="sdate">START DATE</label>
    </div>
    <div class="col-lg-3">
      <label for="edate">END DATE</label>
    </div>
    <div class="col-lg-2">

      <label for="pno"class="text-center">PHONE NUMBER</label>
    </div>
    <div class="col-lg-4">

      <label for="tType"class="text-center">Transaction Type. (Optional)</label>
    </div>
  </div>
  </div>

  <div class="row form-group">
    <div class="col-lg-3">
      <input type="date" id="sdate" required name="sDate" placeholder="Name" class="form-control form-control-lg" />
    </div>
    <div class="col-lg-3">
      <input type="date" id="edate" required name="eDate" placeholder="End Date" class="form-control form-control-lg" />
    </div>
    <div class="col-lg-2">
      <input type="number" id="pno" required name="pno" placeholder="Phone Number" class="form-control-lg form-control" />
  </div>
  <div class="col-lg-4">
    <select id="tType" class="form-control form-control-lg">
      <option selected value="all">Kindly select Transfer type</option>
      <option value="in">Transfer In</option>
      <option value="out">Transfer Out</option>
      <option value="all">All Transfer</option>
    </select>
  </div>
  </div>


<br />
  <div class="row form-group" >
    <div class="col-lg-12">
      <button type="submit" value="Search" class="btn  btn-lg w-100 btn-outline-success"><span>Search</span></button>
    </div>
  </div>

  <div id="spinner">
    <img src="/images/Spinner.gif" height="100px" />
  </div>
  
  <div class="row form-group" id="errSec">
    <div class="text-center alert alert-danger col-lg-12">
      <p id="errMsg"></p>
    </div>
  </div>
    </div>
    <div id="rightSec" class="col-lg-12 table100 ver6 m-b-110 container">
    
    </div>
    </div>

</form>
  </section>
  </main>
  <script src="/js/app.js"></script>
    <!--===============================================================================================-->	
	<script src="/vendor/jquery/jquery-3.2.1.min.js"></script>
<!--===============================================================================================-->
	<script src="/vendor/bootstrap/js/popper.js"></script>
	<script src="/vendor/bootstrap/js/bootstrap.min.js"></script>
<!--===============================================================================================-->
	<script src="/vendor/select2/select2.min.js"></script>
<!--===============================================================================================-->
	<script src="/js/main.js"></script>
</body>
</html>