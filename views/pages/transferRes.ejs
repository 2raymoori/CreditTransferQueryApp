<html>
<head>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
  <!--===============================================================================================-->	
	<link rel="icon" type="image/png" href="/images/icons/favicon.ico"/>
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/vendor/bootstrap/css/bootstrap.min.css">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/vendor/animate/animate.css">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/vendor/select2/select2.min.css">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/vendor/perfect-scrollbar/perfect-scrollbar.css">
<!--===============================================================================================-->
	<link rel="stylesheet" type="text/css" href="/css/util.css">
	<link rel="stylesheet" type="text/css" href="/css/main.css">
	<link rel="stylesheet" type="text/css" href="/css/custom.css">
<!--===============================================================================================-->
  <style>
    body{
      background-color:white;
    }
  </style>
</head>
<body>
  <main>
    <section class="container">
    <div id="imgCont">
  <div>
    <img src="/images/index.png"/>
  </div>

</div>
      <h1 id="heading" class="text-center">SEARCH RESULT FOR PHONE <%=pno %></h1>
      <hr/>
      <div id="detailSec">
        <span class="rep"><b>StartDate :</b> <%=sDate %></span><br />
        <span class="rep"><b>End Date :</b><%= eDate%></span><br/>
        <span class="rep"><b>Number Queried :</b><%=pno%></span>
      </div>

  <%if(pValid == 0){%>
      <div class="alert alert-danger text-center">
        <p>Sorry The Phone to query must be a valid qcell number</p>
      </div>

  <%}else if(exceedPeriod == 1){%>
      <div class="alert alert-danger text-center">
        <p>Sorry... To Avoid CPU Overhead, Kindly run a 30 days query period.Thank you.</p>
      </div>
  <%}else if(senderList.length == 0){%>
  <div class="alert alert-danger text-center" role="alert">
    <p>SORRY THERE IS NO TRANSFER FOR THIS NUMBER (<%=pno %>) BETWEEN THE ABOVE MENTIONED PERIOD.</p>
   <p>Thank you. </p>
  </div>
<%} else{%>
        <div class="table100 ver6 m-b-110 container">
        <table data-vertable="ver6">
          <thead>
            <tr class="row100 head">
              <th class="column100 column1" data-column="column8">TId</th>
              <th class="column100 column2" data-column="column8">Sender</th>
              <th class="column100 column3" data-column="column8">REceiver</th>
              <th class="column100 column4" data-column="column8">Amount Sent (D)</th>
              <th class="column100 column5" data-column="column8">Transaction Date</th>
              <th class="column100 column6" data-column="column8">Status</th>
            </tr>
          </thead>
          <tbody>
            <% for(var i = 0; i<senderList.length; i++) {%>
    <tr class="row100">
    <td class="column100 column1" data-column="column1" ><%=tIds[i]%></td>
    <td class="column100 column2" data-column="column2">
        <%if(flags[i] ===0 ){%>
          <%=receiverList[i] %>
        <%}else{%>
            <b><u><%=senderList[i] %></u></b>
        <%}%>
    </td>
    <td class="column100 column3" data-column="column3">
        <%if(flags[i] == 0 ){%>
          <b><u><%=senderList[i] %></u></b>
        <%}else{%>
            <%=receiverList[i] %>
        <%}%>
    </td>
      <td class="column100 column4" data-column="column4"><%=amountList[i] %></td>
      <td class="column100 column5" data-column="column5"><%=tDateList[i] %></td>
      <td class="column100 column6" data-column="column6">
          <%if(flags[i] == 0 ){%>
          Transfer in
          <%}else{%>
            Transfer Out
          <%}%>
      </td>
    </tr>
  <%}%>

          </tbody>
        </table>
      </div>

  
<%} %>
 
    </section>
  </main>
  <!--===============================================================================================-->	
	<script src="/vendor/jquery/jquery-3.2.1.min.js"></script>
<!--===============================================================================================-->
	<script src="/vendor/bootstrap/js/popper.js"></script>
	<script src="/vendor/bootstrap/js/bootstrap.min.js"></script>
<!--===============================================================================================-->
	<script src="/vendor/select2/select2.min.js"></script>
<!--===============================================================================================-->
	<script src="/js/main.js"></script>
</body>
</html>